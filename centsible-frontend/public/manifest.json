{
  "manifest_version": 3,
  "name": "Centsible",
  "version": "1.0",
  "description": "We make it make cents.",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "sidePanel",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "icons": {
    "32": "default.jpg"
  }
    
}
